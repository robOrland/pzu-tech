generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum TicketStatus {
  PENDENTE
  EM_ANALISE
  RESOLVIDO
}

model Ticket {
  id          String       @id @default(uuid())
  category    String
  description String
  photoUrl    String?
  address     String
  status      TicketStatus @default(PENDENTE)
  createdAt   DateTime     @default(now())
  updatedAt   DateTime     @updatedAt

  @@map("tickets")
}